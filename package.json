{
  "name": "qmoi-alpha-ai",
  "version": "1.0.10",
  "description": "Quantum Master Orchestrator Intelligence (QMOI) — Full automation for multi-platform builds and releases.",
  "author": "Victor Kwemoi",
  "license": "MIT",
  "type": "module",
  "main": "main.js",
  "scripts": {
    "preinstall": "node scripts/install-or-clean.mjs",
    "safe-install": "node scripts/install-or-clean.mjs",
    "install-clean": "node scripts/install-or-clean.mjs",
    "clean:full": "node scripts/install-or-clean.mjs --force-clean",
    "start": "node main.js",
    "dev": "npm run cleanup && node scripts/dev-launcher.js",
    "cleanup": "node scripts/cleanup-before-build.js",
    "convert:require-to-import": "node scripts/convert-require-to-import.mjs .",
    "convert:require-to-import:dry-run": "node scripts/convert-require-to-import.mjs . --dry-run",
    "fix:all": "npm run convert:require-to-import:dry-run && npm run convert:require-to-import && eslint . --fix && prettier --write .",
    "qmoi:always-fix-all": "npm run fix:all && npm run lint:strict && npm run test:autofix",
    "lint": "eslint .",
    "lint:strict": "eslint . --max-warnings=0",
    "test": "node scripts/run-tests.js || echo '⚠️ No tests implemented'",
    "test:autofix": "eslint . --fix && eslint . --max-warnings=0",
    "build:web": "vite build",
    "build:electron:win": "electron-builder --win --x64 --ia32",
    "build:electron:linux": "electron-builder --linux --arm64 --x64",
    "build:electron:mac": "electron-builder --mac",
    "build:electron:all": "npm run build:electron:win && npm run build:electron:linux && npm run build:electron:mac",
    "build:android": "python scripts/qmoi_app_builder.py --platform android --out dist/qmoi_ai.apk --icon public/icon.png",
    "build-windows-installer": "python scripts/qmoi_app_builder.py --build-windows-installer",
    "qa:autotest": "python scripts/qmoi-install-autotest.py",
    "links:test": "python scripts/qmoi-download-link-tester.py",
    "autofix-errors": "node scripts/autofix-errors.mjs"
  },
  "dependencies": {
    "archiver": "^6.0.0",
    "axios": "^1.6.8",
    "bitget-api": "^1.1.0",
    "dotenv": "^16.4.5",
    "form-data": "^4.0.0",
    "gh-release": "^7.0.0",
    "googleapis": "^144.0.0",
    "node-fetch": "^3.3.2",
    "rimraf": "^5.0.1",
    "simple-git": "^3.20.0",
    "vite": "^5.2.8"
  },
  "devDependencies": {
    "@babel/parser": "^7.22.9",
    "@types/web": "^0.0.260",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "chalk": "^5.3.0",
    "electron-builder": "^24.6.0",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.5",
    "marked": "^12.0.2",
    "npm-force-resolutions": "^0.0.10",
    "prettier": "^3.2.5",
    "puppeteer": "^21.5.1",
    "recast": "^0.23.5",
    "shelljs": "^0.8.5"
  },
  "resolutions": {
    "source-map": "0.7.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "optionalDependencies": {
    "windows-shortcuts": "^2.1.0"
  }
}
