{
  "name": "qmoi-alpha-ai",
  "version": "1.0.0",
  "description": "QMOI Alpha AI - Comprehensive AI System with Friendship Enhancement",
  "main": "main.js",
  "build": {
    "appId": "com.qmoi.ai",
    "productName": "qmoi ai",
    "files": [
      "main.js",
      "public/**/*",
      "package.json"
    ],
    "directories": {
      "output": "Qmoi_apps/windows"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "icon": "public/favicon.ico"
    }
  },
  "scripts": {
    "start": "next start",
    "dev": "next dev",
    "build": "next build",
    "build:prod": "next build && npm run build:optimize",
    "build:optimize": "node scripts/build-optimizer.js",
    "test": "jest --watchAll=false --coverage --coverageReporters=text --coverageReporters=lcov --coverageReporters=cobertura",
    "test:watch": "jest --watch",
    "test:ui": "playwright test",
    "test:e2e": "cypress run",
    "test:coverage": "npm test -- --coverage --coverageReporters=text --coverageReporters=lcov --coverageReporters=cobertura",
    "test:performance": "node scripts/performance-test.js",
    "test:debug": "node scripts/debug-tests.js",
    "eject": "react-scripts eject",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src/ --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit --skipLibCheck",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "auto:setup": "node scripts/auto-setup.js",
    "auto:fix": "node scripts/auto-setup.js --fix-all",
    "health:check": "node scripts/health-check.js",
    "cleanup": "node scripts/cleanup.js",
    "docs:generate": "node scripts/docs-generator.js",
    "docs:deploy": "node scripts/docs-deploy.js",
    "security:scan": "node scripts/security-scan.js",
    "lighthouse:test": "node scripts/lighthouse-test.js",
    "vercel:auto-fix": "node scripts/auto-vercel-fix.js",
    "gitlab:automation": "node scripts/gitlab-automation.js",
    "gitlab:push": "node scripts/gitlab-push-automation.js --push-only",
    "gitlab:full-pipeline": "node scripts/gitlab-automation.js --full-pipeline",
    "gitlab:push-pipeline": "node scripts/gitlab-push-automation.js --full-pipeline",
    "gitlab:deploy": "node scripts/gitlab-automation.js --deploy-only",
    "gitlab:notify": "node scripts/gitlab-automation.js --notify-only",
    "gitlab:recovery": "node scripts/gitlab-automation.js --recovery-only",
    "gitlab:health": "node scripts/gitlab-automation.js --health-check",
    "gitlab:fix": "node scripts/gitlab-automation.js --fix-only",
    "gitlab:merge-request": "node scripts/gitlab-push-automation.js --merge-request",
    "gitlab:tag-release": "node scripts/gitlab-push-automation.js --tag-release",
    "gitlab:commit": "node scripts/gitlab-push-automation.js --commit-only",
    "gitpod:notify": "node scripts/gitpod-notification-service.js",
    "gitpod:workspace-started": "node scripts/gitpod-notification-service.js --workspace-started",
    "gitpod:workspace-stopped": "node scripts/gitpod-notification-service.js --workspace-stopped",
    "gitpod:workspace-cloned": "node scripts/gitpod-notification-service.js --workspace-cloned",
    "gitpod:workspace-synced": "node scripts/gitpod-notification-service.js --workspace-synced",
    "gitpod:workspace-error": "node scripts/gitpod-notification-service.js --workspace-error",
    "gitpod:qmoi-integration": "node scripts/gitpod-notification-service.js --qmoi-integration",
    "gitpod:monitor": "node scripts/gitpod-notification-service.js --monitor-workspaces",
    "gitpod:start": "node scripts/gitpod-notification-service.js --start-workspace",
    "gitpod:stop": "node scripts/gitpod-notification-service.js --stop-workspace",
    "gitpod:clone": "node scripts/gitpod-notification-service.js --clone-workspace",
    "gitpod:sync": "node scripts/gitpod-notification-service.js --sync-workspace",
    "json:fix-all": "node scripts/json-config-fixer.js --fix-all",
    "json:fix-package": "node scripts/json-config-fixer.js --fix-package",
    "json:fix-tsconfig": "node scripts/json-config-fixer.js --fix-tsconfig",
    "json:fix-jest": "node scripts/json-config-fixer.js --fix-jest",
    "json:fix-eslint": "node scripts/json-config-fixer.js --fix-eslint",
    "json:validate": "node scripts/json-config-fixer.js --validate",
    "github:fallback": "node scripts/github-fallback.js --fallback-pipeline",
    "github:sync-to": "node scripts/github-fallback.js --sync-to-github",
    "github:sync-from": "node scripts/github-fallback.js --sync-from-github",
    "github:setup": "node scripts/github-fallback.js --setup-github",
    "github:monitor": "node scripts/github-fallback.js --monitor-platforms",
    "github:check-gitlab": "node scripts/github-fallback.js --check-gitlab",
    "github:check-github": "node scripts/github-fallback.js --check-github",
    "github:notify": "node scripts/github-fallback.js --notify",
    "qmoi:setup": "npm run auto:setup",
    "qmoi:fix": "python scripts/qmoi-error-handler.py",
    "qmoi:test": "npm test && npm run test:ui && npm run test:e2e",
    "qmoi:build": "npm run build && npm run build:prod",
    "qmoi:deploy": "npm run gitlab:deploy",
    "qmoi:notify": "python scripts/qmoi-notifications.py",
    "qmoi:health": "python scripts/qmoi-health-monitor.py",
    "qmoi:recovery": "npm run gitlab:recovery",
    "qmoi:full": "npm run qmoi:setup && npm run qmoi:test && npm run qmoi:build && npm run qmoi:deploy && npm run qmoi:notify",
    "qmoi:push": "npm run gitlab:push-pipeline",
    "qmoi:ci": "npm run gitlab:full-pipeline",
    "qmoi:all": "python scripts/qmoi-master-automation.py",
    "qmoi:gitpod": "npm run gitpod:monitor && npm run gitpod:qmoi-integration",
    "qmoi:json-fix": "npm run json:fix-all",
    "qmoi:github-fallback": "npm run github:fallback",
    "qmoi:platform-monitor": "npm run github:monitor && npm run gitpod:monitor",
    "qmoi:comprehensive": "python scripts/qmoi-enhanced-master-automation.py",
    "qmoi:auto-push": "node scripts/qmoi-auto-push.js",
    "qmoi:auto-push-force": "node scripts/qmoi-auto-push.js --force",
    "qmoi:auto-push-backup": "node scripts/qmoi-auto-push.js --backup",
    "qmoi:auto-push-all": "npm run qmoi:auto-push && npm run qmoi:auto-push-force",
    "qmoi:comprehensive-auto": "npm run qmoi:comprehensive && npm run qmoi:auto-push-all",
    "electron:build:win": "electron-builder --win --x64 --ia32 --config.package.json"
  },
  "keywords": [
    "qmoi",
    "ai",
    "friendship",
    "automation",
    "gitlab",
    "vercel",
    "quantum"
  ],
  "author": "QMOI System",
  "license": "MIT",
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@vercel/analytics": "^1.5.0",
    "axios": "^1.11.0",
    "better-sqlite3": "^12.2.0",
    "chart.js": "^4.5.0",
    "child_process": "^1.0.2",
    "chokidar": "^4.0.3",
    "class-variance-authority": "^0.7.1",
    "dockerode": "^4.0.7",
    "express": "^5.1.0",
    "framer-motion": "^12.23.1",
    "fs": "^0.0.1-security",
    "fs-extra": "^11.3.0",
    "lucide-react": "^0.525.0",
    "next": "^14.2.30",
    "next-themes": "^0.4.6",
    "node-fetch": "^3.3.2",
    "node-ssh": "^13.2.1",
    "nodemailer": "^7.0.5",
    "path": "^0.12.7",
    "react": "^18.0.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.0.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "uuid": "^11.1.0",
    "zod": "^4.0.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "cypress": "^14.5.1",
    "electron": "^37.2.3",
    "electron-builder": "^26.0.12",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "playwright": "^1.54.1",
    "rimraf": "^5.0.1",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thealphakenya/Alpha-Q-ai.git"
  },
  "bugs": {
    "url": "https://github.com/thealphakenya/Alpha-Q-ai/issues"
  },
  "homepage": "https://github.com/thealphakenya/Alpha-Q-ai#readme"
}
