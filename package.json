{
  "name": "qmoi-enhanced",
  "version": "1.0.0",
  "description": "QMOI Enhanced Always Fix All System with Comprehensive Automation Tests",
  "main": "scripts/qmoi-always-fix-all.js",
  "scripts": {
    "start": "node scripts/qmoi-always-fix-all.js",
    "fix": "node scripts/qmoi-always-fix-all.js",
    "fix:all": "node scripts/qmoi-always-fix-all.js --comprehensive",
    "fix:test": "node scripts/qmoi-always-fix-all.js --test",
    "fix:monitor": "node scripts/qmoi-always-fix-all.js --monitor",
    "test": "node scripts/qmoi-automation-autotests.js",
    "test:all": "node scripts/qmoi-automation-autotests.js --all",
    "test:unit": "jest --testPathPattern=**/*.test.js --coverage",
    "test:integration": "jest --testPathPattern=**/*.integration.test.js --coverage",
    "test:e2e": "cypress run --spec '**/*.e2e.test.js'",
    "test:performance": "k6 run tests/performance/*.perf.test.js",
    "test:security": "npm audit && snyk test",
    "test:accessibility": "axe-core --config tests/accessibility/a11y.config.js",
    "test:compatibility": "testcafe chrome,firefox,safari,edge tests/compatibility/",
    "test:load": "artillery run tests/load/load-test.yml",
    "test:stress": "artillery run tests/stress/stress-test.yml",
    "test:regression": "jest --testPathPattern=**/*.regression.test.js",
    "test:smoke": "jest --testPathPattern=**/*.smoke.test.js --bail",
    "test:sanity": "jest --testPathPattern=**/*.sanity.test.js --bail",
    "test:visual": "backstop test --config tests/visual/backstop.json",
    "test:api": "newman run tests/api/qmoi-api.postman_collection.json",
    "test:database": "jest --testPathPattern=**/*.database.test.js",
    "test:network": "jest --testPathPattern=**/*.network.test.js",
    "test:mobile": "appium --config tests/mobile/appium.config.js",
    "test:cross-browser": "testcafe chrome,firefox,safari,edge tests/cross-browser/",
    "test:localization": "jest --testPathPattern=**/*.localization.test.js",
    "test:coverage": "jest --coverage --coverageReporters=html,json,lcov",
    "test:report": "jest --coverage --coverageReporters=html",
    "test:junit": "jest --coverage --coverageReporters=junit",
    "test:json": "jest --coverage --coverageReporters=json",
    "test:screenshots": "cypress run --spec '**/*.e2e.test.js' --screenshot",
    "test:video": "cypress run --spec '**/*.e2e.test.js' --video",
    "test:metrics": "node scripts/generate-test-metrics.js",
    "test:watch": "jest --watch",
    "test:debug": "jest --detectOpenHandles --forceExit",
    "test:ci": "npm run test:all -- --ci --coverage --watchAll=false",
    "test:quick": "npm run test:smoke && npm run test:sanity",
    "test:full": "npm run test:all && npm run test:coverage",
    "test:parallel": "jest --maxWorkers=4",
    "test:sequential": "jest --maxWorkers=1",
    "test:retry": "jest --retryTimes=3",
    "test:timeout": "jest --testTimeout=30000",
    "test:verbose": "jest --verbose",
    "test:silent": "jest --silent",
    "test:update": "jest --updateSnapshot",
    "test:clear": "jest --clearCache",
    "test:list": "jest --listTests",
    "test:coverage:threshold": "jest --coverage --coverageThreshold='{\"global\":{\"statements\":80,\"branches\":70,\"functions\":80,\"lines\":80}}'",
    "test:lint": "eslint tests/ --ext .js,.jsx,.ts,.tsx",
    "test:lint:fix": "eslint tests/ --ext .js,.jsx,.ts,.tsx --fix",
    "test:format": "prettier --write tests/",
    "test:format:check": "prettier --check tests/",
    "test:type": "tsc --noEmit",
    "test:build": "npm run build && npm run test",
    "test:clean": "rm -rf test-results coverage reports artifacts",
    "test:setup": "npm run test:clean && npm install",
    "test:teardown": "npm run test:clean",
    "test:validate": "npm run test:lint && npm run test:format:check && npm run test:type",
    "test:prepare": "npm run test:validate && npm run test:setup",
    "test:complete": "npm run test:prepare && npm run test:full && npm run test:teardown",
    "autotest": "node scripts/qmoi-automation-autotests.js",
    "autotest:all": "node scripts/qmoi-automation-autotests.js --all",
    "autotest:quick": "node scripts/qmoi-automation-autotests.js --quick",
    "autotest:parallel": "node scripts/qmoi-automation-autotests.js --parallel",
    "autotest:coverage": "node scripts/qmoi-automation-autotests.js --coverage",
    "autotest:report": "node scripts/qmoi-automation-autotests.js --report",
    "autotest:ci": "node scripts/qmoi-automation-autotests.js --ci",
    "autotest:watch": "node scripts/qmoi-automation-autotests.js --watch",
    "autotest:debug": "node scripts/qmoi-automation-autotests.js --debug",
    "autotest:validate": "node scripts/qmoi-automation-autotests.js --validate",
    "autotest:setup": "node scripts/qmoi-automation-autotests.js --setup",
    "autotest:teardown": "node scripts/qmoi-automation-autotests.js --teardown",
    "autotest:complete": "npm run autotest:setup && npm run autotest:all && npm run autotest:teardown",
    "dev": "npm run fix:monitor",
    "build": "npm run fix:all && npm run test:all",
    "deploy": "npm run build && npm run test:ci",
    "ci": "npm run fix:all && npm run autotest:complete",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type": "tsc --noEmit",
    "clean": "rm -rf node_modules package-lock.json dist build",
    "install:clean": "npm run clean && npm install",
    "update": "npm update && npm audit fix",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "security": "snyk test && npm audit",
    "security:fix": "snyk wizard && npm audit fix",
    "docs": "jsdoc -c jsdoc.json",
    "docs:serve": "http-server docs/ -p 8080",
    "monitor": "node scripts/qmoi-monitor.js",
    "monitor:start": "pm2 start scripts/qmoi-monitor.js --name qmoi-monitor",
    "monitor:stop": "pm2 stop qmoi-monitor",
    "monitor:restart": "pm2 restart qmoi-monitor",
    "monitor:logs": "pm2 logs qmoi-monitor",
    "monitor:status": "pm2 status",
    "backup": "node scripts/qmoi-backup.js",
    "backup:create": "npm run backup -- --create",
    "backup:restore": "npm run backup -- --restore",
    "backup:list": "npm run backup -- --list",
    "backup:clean": "npm run backup -- --clean",
    "health": "node scripts/qmoi-health-check.js",
    "health:check": "npm run health -- --check",
    "health:fix": "npm run health -- --fix",
    "health:report": "npm run health -- --report",
    "optimize": "node scripts/qmoi-optimize.js",
    "optimize:performance": "npm run optimize -- --performance",
    "optimize:memory": "npm run optimize -- --memory",
    "optimize:disk": "npm run optimize -- --disk",
    "optimize:all": "npm run optimize -- --all",
    "validate": "npm run lint && npm run format:check && npm run type && npm run test:validate",
    "prepare": "npm run validate && npm run test:setup",
    "complete": "npm run prepare && npm run build && npm run test:complete",
    "reset": "npm run clean && npm run install:clean && npm run test:setup",
    "status": "node scripts/qmoi-status.js",
    "status:system": "npm run status -- --system",
    "status:tests": "npm run status -- --tests",
    "status:all": "npm run status -- --all",
    "help": "node scripts/qmoi-help.js",
    "help:fix": "npm run help -- --fix",
    "help:test": "npm run help -- --test",
    "help:all": "npm run help -- --all",
    "qmoi:always-fix-all": "node scripts/qmoi-always-fix-all.js",
    "update:artifact-action": "node scripts/update-artifact-action.js",
    "parallel:setup": "node scripts/qmoi-parallel-setup.js",
    "cloud:env": "node scripts/qmoi-cloud-env-manager.js",
    "hot:reload": "node scripts/qmoi-hot-reload.js",
    "cloud:registry": "node scripts/qmoi-cloud-registry.js",
    "parallel:autotest": "node scripts/qmoi-parallel-autotest.js",
    "slack:notify": "node scripts/qmoi-slack-notify.js",
    "cloud:provider": "node scripts/qmoi-cloud-provider.js",
    "dashboard:server": "node dashboard/server.js",
    "master:website": "node scripts/qmoi_master_website_automation.js"
  },
  "keywords": [
    "qmoi",
    "automation",
    "testing",
    "auto-fix",
    "ci-cd",
    "monitoring",
    "optimization",
    "security",
    "performance",
    "accessibility"
  ],
  "author": "QMOI Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "nodemailer": "^6.9.0",
    "crypto": "^1.0.1",
    "fs": "^0.0.1-security",
    "path": "^0.12.7",
    "child_process": "^1.0.2",
    "ws": "8.18.3"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "cypress": "^13.6.0",
    "artillery": "^2.0.0",
    "testcafe": "^3.6.0",
    "newman": "^6.0.0",
    "appium": "^2.2.0",
    "backstopjs": "^6.2.0",
    "axe-core": "^4.8.0",
    "snyk": "^1.1234.0",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "typescript": "^4.9.5",
    "jsdoc": "^4.0.0",
    "pm2": "^5.3.0",
    "http-server": "^14.1.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/q-city/qmoi-enhanced.git"
  },
  "bugs": {
    "url": "https://github.com/q-city/qmoi-enhanced/issues"
  },
  "homepage": "https://github.com/q-city/qmoi-enhanced#readme",
  "type": "module"
}