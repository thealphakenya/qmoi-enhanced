        {
          "src": "/icons/chat-icon-96x96.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Gaming Hub",
      "short_name": "Games",
      "description": "Access gaming platform",
      "url": "/gaming",
      "icons": [
        {
          "src": "/icons/game-icon-96x96.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Revenue Dashboard",
      "short_name": "Revenue",
      "description": "View revenue analytics",
      "url": "/revenue",
      "icons": [
        {
          "src": "/icons/revenue-icon-96x96.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Voice Control",
      "short_name": "Voice",
