continue # .npmrc â€” QMOI Alpha AI Configuration

# Node-gyp build config for native modules on Windows
msvs_version=2022

# Smooth legacy deps installs (Electron/native)
legacy-peer-deps=true

# Use local node_modules/.bin for scripts
scripts-prepend-node-path=true

# Suppress audit output (especially for CI logs)
audit=false

# Avoid generating or updating package-lock.json
package-lock=false

# Local cache path (useful in CI)
cache=.npm-cache

# Auto-confirm prompts
yes=true

# Reduce npm log output to errors only to suppress config warnings
loglevel=error
